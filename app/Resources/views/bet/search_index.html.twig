            type_delay: 350,
            select_column_index: 0
        };
    </script>
    {{ parent() }}
    {%  javascripts '%kernel.root_dir%/Resources/public/js/script.js'%}
<script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
