{% extends 'base.html.twig' %}

{% block body %}
    <div class="container">
        {% form_theme form 'form/form_layout.html.twig' %}
            {{ form_start(form) }}
            {{ form_widget(form) }}

        <div id="result_div">

                <table><tbody></tbody></table>

        </div>
        {{ form_end(form) }}
        <div class="row">
            <div class="eight columns">Test</div>
            <div class="four columns">sd</div>
        </div>
    </div>
{% endblock %}

{% block javascripts %}
